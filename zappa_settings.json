{
    "dev": {
        "app_function": "app.__init__.app", 
        "s3_bucket": "zappa-vcal-dyna-dev",
        "events": [
          {
           "function": "app.common.jobs.unbooked_dates", // The function to execute
           "expression": "cron(0 18 ? * FRI *)" // When to execute it (in cron or rate format)
       },
        {
           "function": "app.common.jobs.weekly_reminder", // The function to execute
           "expression": "cron(0 18 ? * FRI *)" // Every friday 6pm UTC
       }]
    ,
      "manage_roles": false,
      "role_name": "ZappaLambdaExecution",
      "binary_support" : false,
      "cors": {
        "allowed_headers": [
          "accept",
          "accept-encoding",
          "content-type",
          "dnt",
          "origin",
          "user-agent",
          "x-csrftoken",
          "X-Requested-With",
          "Location",
          "Status",
          "X-Frame-Options",
          "Set-Cookie",
          "Authorization",
          "Access-Control-Allow-Origin",
          "Access-Control-Allow-Headers",
          "Access-Control-Allow-Methods"
        ],
      "allowed_origin": "*",
      "allowed_methods" :  ["DELETE", "GET", "HEAD", "OPTIONS", "PATCH", "POST", "PUT"]
      }
    }
}